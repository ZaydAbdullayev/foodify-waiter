{
  "extends": [
    "development"
  ],
  "hints": {
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/paint": "off",
    "highest-available-document-mode": "off",
    "manifest-file-extension": "off",
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 12.5"
  ]
}